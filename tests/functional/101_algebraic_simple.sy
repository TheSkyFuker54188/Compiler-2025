// 测试代数化简的简单例子
int main(){
    int a, b, c, d;
    a = 5;
    b = 0;
    c = 1;
    d = 2;
    
    // 这些操作应该被代数化简
    int result1 = a + 0;      // a + 0 = a
    int result2 = 0 + a;      // 0 + a = a  
    int result3 = a * 1;      // a * 1 = a
    int result4 = 1 * a;      // 1 * a = a
    int result5 = a * 0;      // a * 0 = 0
    int result6 = 0 * a;      // 0 * a = 0
    int result7 = a - 0;      // a - 0 = a
    int result8 = a / 1;      // a / 1 = a
    int result9 = a * 2;      // a * 2 = a + a (强度折减)
    
    return result1 + result2 + result3 + result4 + result5 + result6 + result7 + result8 + result9;
}
